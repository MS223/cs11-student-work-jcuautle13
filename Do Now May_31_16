class Time(object):
 def __init__(self, hour, minute, second):
     self.hour = hour
     self.minute = minute
     self.second = second

def __str__(self):
    return str(self.hour) + ":" + str(self.minute) + ":" + str(self.second)

def add(self,kill):
    return str(self.hour + kill.hour) + ":" + str(self.minute + kill.minute) + ":" + str(self.second + kill.second)

time1 = Time(5, 32, 0)
time2 = Time(23, 11, 11)

print time1
print time2

print time1 + time2
